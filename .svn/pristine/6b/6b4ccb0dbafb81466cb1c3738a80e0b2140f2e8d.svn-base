function onclickServer(n){
  if(eval("frmHome.flexServerImg"+n).skin=="sknFlexServerN"){
    eval("frmHome.flexServerImg"+n).skin="sknFlexServerF"
    eval("frmHome.flexServerLbl"+n).skin="sknLbl3F"
  }
  else{
    eval("frmHome.flexServerImg"+n).skin="sknFlexServerN"
    eval("frmHome.flexServerLbl"+n).skin="sknLbl3"
  }
}

